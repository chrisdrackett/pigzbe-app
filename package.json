{
  "name": "pigzbe-app",
  "version": "0.1.0",
  "main": "node_modules/expo/AppEntry.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pigzbe/pigzbe-app.git"
  },
  "scripts": {
    "test": "jest",
    "postinstall": "rm -rf node_modules/gl-matrix/.babelrc",
    "desktop": "run-p serve:webpack start:electron",
    "launch:electron": "electron './desktop/main.js'",
    "serve:webpack": "webpack-dev-server -d --config './desktop/webpack.config.js' --inline --hot --colors --host '0.0.0.0' --useLocalIp",
    "start:electron": "wait-on 'http://localhost:8080' && npm run launch:electron",
    "build:webpack": "webpack -p --config './desktop/webpack.config.js'"
  },
  "dependencies": {
    "@expo/browser-polyfill": "^0.0.0-alpha.6",
    "@pigzbe/react-native-stellar-sdk": "^1.0.2",
    "babel-polyfill": "^6.26.0",
    "expo": "^26.0.0",
    "path": "^0.12.7",
    "pixi.js": "^4.7.3",
    "react": "16.3.0-alpha.1",
    "react-dom": "^16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-26.0.0.tar.gz",
    "react-native-web": "^0.5.4",
    "react-navigation": "^1.5.9",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "stellar-sdk": "^0.8.0",
    "url": "^0.11.0",
    "usfl": "^3.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-native-web": "^0.5.4",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "npm-run-all": "^4.1.2",
    "url-loader": "^1.0.1",
    "wait-on": "^2.1.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
