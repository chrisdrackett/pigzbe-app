    "react-dom": "^16.2.0",
    "react-native": "0.52.0",
    "react-native-vector-icons": "^4.5.0",
    "react-native-web": "^0.5.1",
    "react-navigation": "^1.5.1",
    "stellar-sdk": "^0.8.0",
    "url": "^0.11.0",
    "usfl": "^3.2.3"
